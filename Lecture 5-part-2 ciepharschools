#include <iostream>
using namespace std;

int main() {
   int a[4];
 for (int i = 0; i <5 ;i++) {
        cout >> a[i];
}

 for (int i = 0; i <5; i++) {
        cout << a[i];
}

return 0;
}
////////////////////////////////////


#include <iostream>
using namespace std;

int main() {
   int a[9];
 for (int i = 0; i <10 ;i++) {
        cout >> a[i];
}

 for (int i = 9; i >=0; i--) {
        cout << a[i];
}

return 0;
}
////////////////////////////


#include <iostream>
using namespace std;

int main() {
   int a[]={1,2,3,4,5};
 for (int i = 0; i < a.size(); i++) {
        cout << a[i] << " ";
    }

