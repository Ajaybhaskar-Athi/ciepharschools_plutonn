#include<iostream>
using namespace std;
int main(){
cout<<"enter inetegr";
cin>>a;
switch (a){
case 1:
cout<<'THe value of a is 1";
break;
case 2:
cout<<'THe value of a is 2";
break;
default:
cout<<"default wiill be printed";
break;
}
return 0;
}

