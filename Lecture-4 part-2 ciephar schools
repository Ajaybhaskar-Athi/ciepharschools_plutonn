#include<iostream>
using namespace std;
int main(){
int a,b,c,d;
cin>>a>>b>>c>>d;
if(a>b && c>d){
cout<<"haha";
}
else If(a>b && c<d){
cout<<"hehe";
}
elseif(a<b && c>d){
cout<<"HUHU";
}
return 0;
}
