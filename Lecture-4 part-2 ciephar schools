#include<iostream>
using namespace std;
int main(){
int marks;
cin>>marks;
if(marks>90){
cout<<"A";
}
else if (marks<90 && marks >80){
cout<<"B";
}
else if (marks>70)
{
cout<<"Pass";
}
else{

cout<<"fail";
}
}
