#include<iostream>
using namespace std;
int main(){
float a;
cin>>a;
if(a>0.01){
cout<<"the condition in if statement is correct"<<endl;
}
float b,c;
cin>>b>>c;
if(b>0.0.1 && c>b){
cout<<"the condition in if statement is correct"<<endl;
}
if(b>0.0.1 || c>b){
cout<<"the condition in if statement is correct"<<endl;
}

/// but here when condition not true it was empty console 
//here we comes the concept of ELSE statement;

}

